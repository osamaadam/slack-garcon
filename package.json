{
  "name": "garcon",
  "version": "1.0.0",
  "description": "A playful TypeScript Slack bot powered by Gemini AI",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "deploy:lambda": "serverless deploy",
    "remove:lambda": "serverless remove",
    "logs:lambda": "serverless logs -f slack_garcon -t"
  },
  "keywords": [
    "slack",
    "bot",
    "gemini",
    "ai",
    "typescript"
  ],
  "author": "Osama Adam <osamaadamme@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.929.0",
    "@google/genai": "^1.29.0",
    "@slack/bolt": "^3.17.1",
    "dotenv": "^16.4.5",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.157",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "aws-lambda": "^1.0.7",
    "eslint": "^8.56.0",
    "serverless": "^3.40.0",
    "serverless-plugin-typescript": "^2.1.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
